<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dperezS5.Page2">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="INSERTAR ESTUDIANTE0" FontSize="20" HorizontalTextAlignment="Center" />
            <Entry x:Name="txtCodigo" Placeholder="Ingrese codigo" Keyboard="Numeric"/>
            <Entry x:Name="txtNombre" Placeholder="Ingrese nombre" />
            <Entry x:Name="txtApellido" Placeholder="Ingrese apellido" />
            <Entry x:Name="txtEdad" Placeholder="Ingrese Edad" Keyboard="Numeric"/>
            <Button x:Name="btnInsertar" Text="INSERTAR" Clicked="btnInsertar_Clicked"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>